{
  "extends": [
    "@cartant/eslint-config",
    "@cartant/eslint-config-etc",
    "@cartant/eslint-config-rxjs"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "comments": true,
    "ecmaVersion": 2019,
    "project": "tsconfig.json",
    "sourceType": "module"
  },
  "root": true,
  "rules": {
    "etc/deprecation": ["off"]
  }
}
